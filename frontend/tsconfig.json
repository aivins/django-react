{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "moduleResolution": "node",
    "lib": ["dom", "es5", "es6"],
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "sourceMap": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules/**/*"]
}
