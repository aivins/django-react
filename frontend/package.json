{
  "license": "UNLICENSED",
  "scripts": {
    "start": "yarn run relay && parcel serve index.html",
    "django": "yarn run relay && parcel serve --hmr-port 9999 index-django.html",
    "typecheck": "yarn run relay && tsc --noEmit",
    "relay": "relay-compiler --language typescript --schema schema.graphql --src ./src/ $@",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write 'src/**/*.(js|jsx|ts|tsx)'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "barrelsby": "barrelsby --delete --exportDefault --exclude '__generated__' $@",
    "generate-barrels": "yarn run barrelsby ---directory src/components && yarn run barrelsby ---directory src/containers && yarn run barrelsby --directory src/lib"
  },
  "dependencies": {
    "@apollo/client": "^3.4.7",
    "bootstrap": "5.0.2",
    "graphql": "^15.5.3",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.0-beta.4",
    "react-bootstrap-icons": "^1.5.0",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-relay": "^12.0.0",
    "react-router-dom": "^5.2.0",
    "react-table": "^7.7.0",
    "relay-runtime": "^12.0.0",
    "src": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/react-relay": "^11.0.2",
    "@types/relay-runtime": "^11.0.4",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-relay": "^12.0.0",
    "barrelsby": "^2.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-relay": "^1.8.2",
    "parcel": "^2.0.0-rc.0",
    "prettier": "^2.4.0",
    "relay-compiler": "^12.0.0",
    "relay-compiler-language-typescript": "^14.1.0",
    "typescript": "^4.4.3"
  }
}
